<h1>Users</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th></th>
        <th>名前</th>
        <th>性別</th>
        <th>レベル</th>
        <th>地域</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(100,100) %></td>
        <td><%= link_to user.name, user_path(user) %></td>
        <td><%= user.level %></td>
        <th><%= user.location %></th>
      </tr>
      <% end %>
    </tbody>
  </table>